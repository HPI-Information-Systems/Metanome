<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">

<module>
    <inherits name="com.google.gwt.user.User"/>
    <source path="algorithm_integration">
        <include name="AlgorithmConfigurationException.java"/>
        <include name="AlgorithmExecutionException.java"/>
        <include name="ColumnCombination.java"/>
        <include name="ColumnIdentifier.java"/>
    </source>
    <source path="algorithm_integration/configuration">
        <include name="ConfigurationSettingBoolean.java"/>
        <include name="ConfigurationSettingCsvFile.java"/>
        <include name="ConfigurationSettingDataSource.java"/>
        <include name="ConfigurationSettingSqlIterator.java"/>
        <include name="ConfigurationSettingString.java"/>
        <include name="ConfigurationSettingInteger.java"/>
        <include name="ConfigurationSettingListBox.java"/>
        <include name="ConfigurationSpecification.java"/>
        <include name="ConfigurationSpecificationBoolean.java"/>
        <include name="ConfigurationSpecificationCsvFile.java"/>
        <include name="ConfigurationSpecificationSqlIterator.java"/>
        <include name="ConfigurationSpecificationString.java"/>
        <include name="ConfigurationSpecificationListBox.java"/>
        <include name="ConfigurationSpecificationInteger.java"/>
        <include name="DbSystem.java"/>
    </source>
    <source path="algorithm_integration/result_receiver">
        <include name="OmniscientResultReceiver.java"/>
        <include name="BasicStatisticsResultReceiver.java"/>
        <include name="FunctionalDependencyResultReceiver.java"/>
        <include name="InclusionDependencyResultReceiver.java"/>
        <include name="UniqueColumnCombinationResultReceiver.java"/>
        <include name="CouldNotReceiveResultException.java"/>
    </source>
    <source path="algorithm_integration/results">
        <include name="Result.java"/>
        <include name="UniqueColumnCombination.java"/>
        <include name="FunctionalDependency.java"/>
        <include name="InclusionDependency.java"/>
        <include name="BasicStatistic.java"/>
    </source>
    <source path="algorithm_loading">
        <include name="AlgorithmLoadingException.java"/>
    </source>
    <source path="results_db">
        <include name="Algorithm.java"/>
        <include name="AlgorithmContentEquals.java"/>
    </source>
</module>